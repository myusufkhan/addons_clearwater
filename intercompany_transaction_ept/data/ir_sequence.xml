<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="1">

		<!-- Sequences -->
		<!-- ICT -->
		<record id="ir_sequence_inter_company_transaction"
			model="ir.sequence">
			<field name="name">Inter Location Transfer</field>
			<field name="prefix">ILT</field>
			<field name="padding">3</field>
			<field name="number_increment">1</field>
			<field name="number_next">1</field>
			<field name="company_id" ref="base.main_company"></field>
		</record>

		<!-- RICT -->
		<record id="ir_sequence_reverse_inter_company_transaction"
			model="ir.sequence">
			<field name="name">Reverse Inter Location Transaction</field>
			<field name="prefix">RILT</field>
			<field name="padding">3</field>
			<field name="number_increment">1</field>
			<field name="number_next">1</field>
			<field name="company_id" ref="base.main_company"></field>
		</record>

		<!-- INT -->
		<record id="ir_sequence_internal_transfer" model="ir.sequence">
			<field name="name">Internal Transfer</field>
			<field name="prefix">INTILT</field>
			<field name="padding">3</field>
			<field name="number_increment">1</field>
			<field name="number_next">1</field>
			<field name="company_id" ref="base.main_company"></field>
		</record>

		<!-- LOG -->
		<record id="inter_company_process_log_seq" model="ir.sequence">
			<field name="name">Inter Location Transfer LOG</field>
			<field name="prefix">LOG</field>
			<field name="padding">4</field>
			<field name="code">ict_log</field>
			<field name="number_increment">1</field>
			<field name="number_next">1</field>
			<field name="company_id" eval="False" />
		</record>

	</data>
</odoo>