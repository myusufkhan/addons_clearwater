     <record id="view_stock_move_line_dates_add_action_open" model="ir.ui.view">
        <field name="name">stock.move.line.view_stock_move_line_dates_add_action_open</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_stock_move_line_operation_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='location_dest_id']" position="after">
                <field name="mfg_waranty_date"/>
                <field name="rec_d_into_inventory"/>
